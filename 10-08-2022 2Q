#include<stdio.h>
int main()
{
	int num;
	int temp;
	scanf("%d",&num);
	int sum = 0;
	temp = num;
	while(temp)
	{
	     sum += temp % 10;
		 temp = temp / 10;	
    }
    int result = num % sum;
    if(result == 0)
        printf("Harshad Number");
    else
        printf("Not Harshad Number");
    return 0;
}
